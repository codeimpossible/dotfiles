{
    "newFile.relativeTo": "file",
    "newFile.showPathRelativeTo": "project",
    "newFile.defaultFileExtension": ".cs",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Fira Code iScript', 'FiraCode-Retina', Menlo, Monaco, 'Courier New', monospace",
    "window.zoomLevel": 0,
    "terminal.integrated.shell.osx": "/bin/zsh",
